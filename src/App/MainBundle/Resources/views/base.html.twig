<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>
        {% block title %}{% endblock %}
    </title>
    <meta name="description"
          content="{% block meta_description %}{% endblock %}">
    <meta name="keywords"
          content="{% block meta_keywords %}{% endblock %}">

    {% stylesheets '@AppMainBundle/Resources/public/css/*' filter='cssrewrite' output='css/compiled/main.css' %}
    <link media="all" rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}
    {% javascripts
        '@AppMainBundle/Resources/public/js/jquery-2.1.1.min.js'
        '@AppMainBundle/Resources/public/js/bootstrap.min.js'
        '@AppMainBundle/Resources/public/js/jquery.sort.js'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

</head>
<body>
<div class="content">
    {% block main_content %}{% endblock %}
    {% block footer %}{% endblock %}
</div>

        {% javascripts
            '@AppMainBundle/Resources/public/js/main.js'
        %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

{% block footer_js %}{% endblock %}

</body>
</html>